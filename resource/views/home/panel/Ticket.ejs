<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>All Ticket</title>
  <link rel="stylesheet" href="/css/Ticket.css">
  <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
</head>
<body>
  
  <div class="kol">
    <div class="nav">
        <h1 class="logo">ASAS</h1>
        <div class="title">
        <h2 class="titr">Ticket</h2>
        </div>
        <div class="user"><i class='bx bx-user' style='color:#e2dddd'></i><h4><%= auth.user.phone %></h4></div>
        <nav class="ul">
            <ul>
                <li><i class='bx bxs-home-alt-2' id="home-icon"></i><a id="home" href="/">Home</a></li>
                <li><i class='bx bx-plus-medical' id="plus-icon" ></i><a id="plus" href="/user/panel/Tickets/New-Ticket">New Ticket</a></li>
            </ul>
        </nav>
    </div>
    <div class="tab">

        <table>

            <thead>
              <tr>
                <th scope="col">Name</th>
                <th scope="col">Phone</th>
                <th scope="col">Ticket</th>
                <th scope="col">Open</th>
              </tr>
            </thead>
            <% Tickets.forEach(Ticket => { %>

            <tbody>

              <tr>
                <td scope="row"><%= Ticket.name %></td>
                <td><%= Ticket.phone %></td>
                <td><%= Ticket.TicketName %></td>
                <td><a class="del" href="/user/panel/ticket/<%= Ticket._id %>">Open</a></td>
              </tr>

            </tbody>
            <% }) %>

        </table>

        </div>
    </div>
</body>
</html>
 
    


